<?xml version="1.0" encoding="utf-8"?>
<animations xmlns="http://esheep.petrucci.ch/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://esheep.petrucci.ch/ https://raw.githubusercontent.com/Adrianotiger/desktopPet/master/Resources/animations.xsd">

  <header>
    <author>Adriano</author>
    <title>ball</title>
    <petname>Ball</petname>
    <version>1.0</version>
    <info>Simple ball, bouncing from left to right on the screen</info>
    <application>1</application>
    <icon><![CDATA[]]></icon>
  </header>
  <image>
    <tilesx>1</tilesx>
    <tilesy>1</tilesy>
    <png><![CDATA[]]></png>
    <transparency>Magenta</transparency>
  </image>
  <spawns>
    <spawn probability="100" id="1">
      <x>-imageW-10</x>
      <y>(random*(areaH-imageH))/100</y>
      <next probability="100">1</next>
    </spawn>
  </spawns>
  <animations>
    <animation id="1">
      <name>goright</name>
      <start>
        <x>5</x>
        <y>0</y>
        <interval>50</interval>
      </start>
      <end>
        <x>6</x>
        <y>0</y>
        <interval>50</interval>
      </end>
      <sequence repeat="20" repeatfrom="0">
        <frame>0</frame>
        <frame>0</frame>
        <next probability="100">1</next>
      </sequence>
      <border>
        <next probability="100" only="vertical">2</next>
      </border>
    </animation>
    <animation id="2">
      <name>goleft</name>
      <start>
        <x>-5</x>
        <y>0</y>
        <interval>50</interval>
      </start>
      <end>
        <x>-6</x>
        <y>0</y>
        <interval>50</interval>
      </end>
      <sequence repeat="20" repeatfrom="0">
        <frame>0</frame>
        <frame>0</frame>
        <next probability="100">2</next>
      </sequence>
      <border>
        <next probability="100" only="vertical">1</next>
      </border>
    </animation>
  </animations>
  <childs>
  </childs>
</animations> 
